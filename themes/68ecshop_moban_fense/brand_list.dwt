        <script>
            $$(document).ready(function() {
                $$('.content2, .content3,.content4,.content5,.content6').hide();
                $$('.c1 a span').click(function() {
                    var text = $$(this).text();
                    switch (text) {
                    case '蔬菜农户':
                        $$('.content1').show();
                        $$('.content2, .content3,.content4,.content5,.content6').hide();
                        break;
                    case '水果农户':
                        $$('.content2').show();
                        $$('.content1, .content3,.content4,.content5,.content6').hide();
                        break;
                    case '大米农户':
                        $$('.content3').show();
                        $$('.content2, .content1,.content4,.content5,.content6').hide();
                        break;
                    case '禽蛋农户':
                        $$('.content4').show();
                        $$('.content2, .content3,.content1,.content5,.content6').hide();
                        break;
                    case '畜农户':
                        $$('.content5').show();
                        $$('.content2, .content3,.content4,.content1,.content6').hide();
                        break;
                    case '其他农户':
                        $$('.content6').show();
                        $$('.content2, .content3,.content4,.content5,.content1').hide();
                        break;
                    default:
                        $$('.content1').show();
                        $$('.content2, .content3,.content4,.content5,.content6').hide();
                        break;
                    }
                });
            });
