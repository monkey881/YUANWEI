$$('.goods_img').hover(
          function () {
              
              var obj = $$(this).parents('div.goodsbox').find('div.hidden_text');
              var text = obj.text();
              var openObj = $$('#hidden_area');
              var offset = $$(this).offset();
              var top = offset.top;
              var left = offset.left-220;
              openObj.html(text);
              openObj.css('top',top);
              openObj.css('left',left);
              openObj.show();
            },
            function () {
              $$('#hidden_area').hide();
            }
       );
</script>
</body>
</html>
